{
    "login":{
        "type":"object",
        "properties":{
            "username":{
                "type":"string"
            },
            "password":{
                "type":"string"
            }
        },
        "required":["username", "password"]
    },
    "list":{
        "type":"object",
        "properties":{
            "pageSize":{
                "type":"integer"
            },
            "page":{
                "type":"integer"
            },
            "uuid":{
                "type":"string"
            },
            "token":{
                "type":"string"
            }
        },
        "required":["page", "uuid", "token"]
    }
}